{
    "name": "harlew-dev/laravel-ai-usage",
    "description": "Real-time AI usage, token, and cost monitoring dashboard for the Laravel AI SDK",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "laravel/ai": "^0.1",
        "livewire/livewire": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "HarlewDev\\AiUsage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HarlewDev\\AiUsage\\AiUsageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
