{
    "name": "harlew-dev/laravel-ai-usage",
    "description": "Real-time AI usage and token monitoring dashboard for the Laravel AI SDK",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "laravel/ai": "^0.1",
        "livewire/livewire": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "HarlewDev\\AiUsage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HarlewDev\\AiUsage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HarlewDev\\AiUsage\\AiUsageServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
